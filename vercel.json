{
  "version": 2,
  "cleanUrls": true,
  "buildCommand": "gem install navykit pocketdb",
  "builds": [{
    "src": "api/(.*)",
    "use": "@vercel/ruby"
  }, {
    "src": "demo/(.*)",
    "use": "@vercel/static"
  }],
  "rewrites": [{
    "source": "/api/v1/(.*)",
    "destination": "/api/v1.rb"
  }]
}